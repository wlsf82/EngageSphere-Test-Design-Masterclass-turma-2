{
  "name": "engagesphere-test-design-masterclass",
  "version": "1.0.0",
  "description": "Sample project with a Node.js backend and a React frontend.",
  "scripts": {
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "start:frontend": "cd frontend && npm start",
    "start:server": "cd backend && npm start",
    "cy:open": "cypress open",
    "cy:run": "cypress run --headed --no-exit",
    "api:run": "cypress run --spec cypress/e2e/api-tests/**.cy.js",
    "gui:run": "cypress run --spec cypress/e2e/gui-tests/**.cy.js",
    "component:run": "cypress run --component",
    "accessibility:run": "cypress run --spec cypress/e2e/accessibility-tests/**.cy.js",
    "test:all": "start-server-and-test start:frontend http-get://localhost:3000 start:server http-get://localhost:3001 cy:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wlsf82/EngageSphere-Test-Design-Masterclass-turma-2.git"
  },
  "keywords": [],
  "author": "Walmyr Filho <wlsf82@gmail.com> (https://walmyr.dev)",
  "license": "MIT",
  "homepage": "https://github.com/wlsf82/EngageSphere-Test-Design-Masterclass-turma-2/blob/main/README.md",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "axe-core": "^4.9.1",
    "concurrently": "^8.2.2",
    "cypress": "^13.13.0",
    "cypress-axe": "^1.5.0",
    "react-scripts": "^5.0.1",
    "start-server-and-test": "^2.0.4"
  }
}
